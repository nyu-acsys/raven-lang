{
  "name": "raven",
  "displayName": "Raven",
  "description": "Syntax highlighting for the Raven verification language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "publisher": "nyu-acsys",
  "license": "See LICENSE file",
  "repository": {
    "type": "git",
    "url": "https://github.com/nyu-acsys/raven-lang.git"
  },
  "contributes": {
    "languages": [{
      "id": "raven",
      "aliases": ["Raven", "raven"],
      "extensions": [".rav"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "raven",
      "scopeName": "source.raven",
      "path": "./syntaxes/raven.tmLanguage.json"
    }]
  }
}
